generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Profile {
  userKey     String   @id            // unique per agent (we'll use the agent's email)
  bio         String   @default("")
  headshotUrl String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
